<?xml version='1.0' encoding='utf-8'?>
<widget id="org.apache.cordova.myApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>h5s视频平台</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="Orientation" value="landscape" />
        <!-- <icon density="ldpi" src="res/mipmap-ldpi/icon_36_ldpi.png" />  
        <icon density="mdpi" src="res/mipmap-mdpi/icon_48_mdpi.png" />   -->
        <!-- <icon density="hdpi" src="res/mipmap-hdpi/icon_72_hdpi.png" />   -->
        <!-- <icon density="xhdpi" src="res/mipmap-xhdpi/icon_48_xhdpi.png" />  
        <icon density="xxhdpi" src="res/mipmap-xxhdpi/icon_114_xxhdpi.png" />  
        <icon density="xxxhdpi" src="res/mipmap-xxxhdpi/icon_192_xxxhdpi.png"/>   -->
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
